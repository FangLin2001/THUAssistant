<!--pages/learn/index.wxml-->
<view class="container tabBar">
  <van-tabbar active="{{ active }}" active-color="#9932CC" inactive-color="#000" bind:change="onChange">
    <van-tabbar-item wx:for="{{ list }}" wx:key="index" icon="{{ item.icon }}">{{
      item.text
      }}
    </van-tabbar-item>
  </van-tabbar>
</view>
